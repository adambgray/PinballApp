@model List<PinballTourneyApp.Models.User>

<h1>Users</h1>

<table class="table">

    <tr>
        <th>Name</th>
        <th>Email</th>

    </tr>

    @foreach (var user in Model)
    {
    <tr>
        <td>@user.Name</td>
        <td>@user.Email</td>


    </tr>
    }
</table>

@if (Model.Count == 0)
{
<p>No users yet!</p>
}